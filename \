
alias c=clear
alias e=exit
alias lock=i3lock-fancy
alias rag=ranger
alias ra='ranger --choosedir=$HOME/.rangerdir; LASTDIR=`cat $HOME/.rangerdir`; cd "$LASTDIR"'
alias cat=bat
alias grep=rg
alias ec=emacsclient
alias ls=lsd
alias l="lsd -l"
alias la="lsd -ail"
alias lt="lsd --tree"
alias td="tmux detach"
alias ta="tmux attach"
alias pa="ps -aux"
alias pg="pa | grep"

